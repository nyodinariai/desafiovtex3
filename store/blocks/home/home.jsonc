{
  "store.home": { 
    "blocks": [
      "flex-layout.row#menu",
      "flex-layout.row#banner",
      "flex-layout.row#shelf"
    ]
  },

  "flex-layout.row#menu":{
    "children":["flex-layout.col#menu"]
  },

  "flex-layout.col#menu":{
    "children":["tab-layout#home"]
  },

  //Tab layout
  "tab-layout#home":{ 
   "children": [
     "tab-list"
   ]
 },


// Categorias
 "tab-list": {
  "children": [
    "tab-list.item#1",
    "tab-list.item#2",
    "tab-list.item#3",
    "tab-list.item#4",
    "tab-list.item#5"
    ]
},

//Categoria 1

"tab-list.item#1": {
  "props": {
    "tabId": "promocao",
    "label": "PROMOÇÃO",
    "blockClass":"listButton"
  }
},

//Categoria 2
"tab-list.item#2": {
  "props": {
    "tabId": "signos",
    "label": "SIGNOS",
    "blockClass":"listButton"
  }
},

//Categoria 3
"tab-list.item#3": {
  "props": {
    "tabId": "corintians",
    "label": "CORINTIANS",
    "blockClass":"listButton"
  }
},


//Categoria 4
"tab-list.item#4": {
  "props": {
    "tabId": "palmeiras",
    "label": "PALMEIRAS",
    "blockClass":"listButton"
  }
},

//Categoria 5
"tab-list.item#5": {
  "props": {
    "tabId": "guerreiros",
    "label": "GUERREIROS",
    "blockClass":"listButton"
  }
},
   
//Banner

"flex-layout.row#banner":{
  "children":["flex-layout.col#banner"]
},

"flex-layout.col#banner":{
  "children":["slider-layout#banner"],
  "props":{
    "horizontalAlign":"center"
  }
},

// Produtos

"flex-layout.row#shelf":{
  "children":[
    "flex-layout.col#shelf"
  ]
},

"flex-layout.col#shelf":{
  "props":{
    "horizontalAlign":"center"
  },
  "children":["tab-content#home"]
},

// Lista de Produtos
"tab-content#home": {
  "children": [
    "tab-content.item#1",
    "tab-content.item#2",
    "tab-content.item#3",
    "tab-content.item#4",
    "tab-content.item#5"
    ]
},

//Conteudo 1
"tab-content.item#1": {
  "children": [
    "list-context.product-list#promo"
  ],
  "props": {
    "tabId": "promocao"
  }
},



"list-context.product-list#promo": {
  "props": {
    "category": "20", //24
    "hideUnavailableItems": true
  },
  "blocks": ["product-summary.shelf#promo"],
  "children": ["slider-layout#promo"]
},
"product-summary.shelf#promo": {
  "children": [
    "product-summary-name",
    "product-summary-image",
    "product-selling-price",
    "product-summary-buy-button"
  ]
},

"slider-layout#promo":{
  "props": {
    "itemsPerPage": {
      "desktop": 2,
      "tablet": 2,
      "phone": 1
    },
    "infinite": true,
    "fullWidth": false,
    "blockClass": "shelf"
  }

},

//Conteudo 2

"tab-content.item#2": {
  "children": [
    "list-context.product-list#signos"
  ],
  "props": {
    "tabId": "signos"
  }
},

"list-context.product-list#signos": {
  "props": {
    "category": "20/21",
    "hideUnavailableItems": true
  },
  "blocks": ["product-summary.shelf#signos"],
  "children": ["slider-layout#signos"]
},

"product-summary.shelf#signos": {
  "children": [
